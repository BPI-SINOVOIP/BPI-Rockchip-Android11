<AiqCameraConfig>
    <!-- List of maximum moduleId for various camera sensor -->
    <!-- Any Camera listed in CameraId row will report
    hdrmode (as parameter for aiq) up to the value. -->
    <modeConfigList>
        <!-- moduleId must be increasing, hdrmode must be NORMAL or HDR2 or HDR3 -->
        <CameraId  moduleId="0" hdrmode="NORMAL" />
        <CameraId  moduleId="1" hdrmode="HDR2" />
        <!-- moduleId larger than the last entry will not be supported-->
    </modeConfigList>
</AiqCameraConfig>

    <lineGap value="0"/>
    <advanceHeightMax value="999"/>
    <minTopSideBearing value="99"/>
    <minBottomSideBearing value="99"/>
    <yMaxExtent value="99"/>
    <caretSlopeRise value="0"/>
    <caretSlopeRun value="1"/>

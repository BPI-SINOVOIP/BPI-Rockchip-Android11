        arm: {
            neon: {
                // Want arm, not thumb, optimized
                instruction_set: "arm",

                srcs: libvpx_arm_neon_c_srcs + libvpx_arm_neon_asm_srcs,
                local_include_dirs: ["config/arm-neon"],
            },
        },

        arm64: {
            srcs: libvpx_arm64_c_srcs,
            local_include_dirs: ["config/arm64"],

art_cc_binary {
    name: "dexlist",
    defaults: ["art_defaults"],
    host_supported: true,
    srcs: ["dexlist.cc"],
    shared_libs: [
        "libdexfile",
        "libartbase",
        "libbase",
    ],
    apex_available: [
        "com.android.art.release",
        "com.android.art.debug",
    ],
}

art_cc_binary {
    name: "dexlists",
    defaults: [
        "art_defaults",
        "libartbase_static_defaults",
        "libdexfile_static_defaults",
    ],
    host_supported: true,
    srcs: ["dexlist.cc"],
    device_supported: false,
    target: {
        darwin: {
            enabled: false,
        },
        windows: {
            enabled: true,
        },
    },
}

art_cc_test {
    name: "art_dexlist_tests",
    defaults: [
        "art_gtest_defaults",
    ],
    srcs: ["dexlist_test.cc"],
}

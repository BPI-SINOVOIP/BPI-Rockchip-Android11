android_app {
    name: "CarrierConfig",
    srcs: ["src/**/*.java"],
    platform_apis: true,
    certificate: "platform",
    system_ext_specific: true,
    privileged: true,
    required: ["privapp_whitelist_com.android.carrierconfig"],
    optimize: {
        proguard_flags_files: ["proguard.flags"],
    },
    aaptflags: ["--keep-raw-values"],
}

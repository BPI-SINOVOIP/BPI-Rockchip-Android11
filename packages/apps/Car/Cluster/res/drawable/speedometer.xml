        </aapt:attr>
    </path>

    <path
        android:fillColor="#000"
        android:strokeWidth="0.25"
        android:pathData="M2,32
        A30,30 0 1,1 62,32
        A30,30 0 1,1 2,32 Z">
        <aapt:attr name="android:strokeColor">
            <gradient
                android:startX="0"
                android:startY="10"
                android:startColor="#000"
                android:endX="0"
                android:endY="150"
                android:endColor="#000"
                android:centerColor="#DDD"
                android:type="linear"/>
        </aapt:attr>

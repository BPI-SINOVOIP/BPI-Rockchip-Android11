            <gradient
                android:centerX="32"
                android:centerY="32"
                android:gradientRadius="32"
                android:type="radial">
                <item
                    android:color="#FF000000"
                    android:offset="0.0"/>
                <item
                    android:color="#FF000000"
                    android:offset="0.94"/>
                <item
                    android:color="#00000000"
                    android:offset="1.0"/>

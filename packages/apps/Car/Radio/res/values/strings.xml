<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The main application name for the radio. -->
    <string name="app_name">Radio</string>

    <!-- Text to denote the AM band. -->
    <string name="programtype_am_text">AM</string>

    <!-- Text to denote the FM band. -->
    <string name="programtype_fm_text">FM</string>

    <!-- Text to denote the DAB program type (band). -->
    <string name="programtype_dab_text">DAB</string>

    <!-- Text indicating the radio hardware is not supported on this device. -->
    <string name="radio_not_supported_text">Radio not supported</string>

    <!-- Text indicating a fatal error (app service crash or HW failure). -->
    <string name="radio_failure_text">Radio temporarily not available</string>

    <!-- Error message after invalid MediaSession API selection (mainly by other apps,
         like assistant). -->
    <string name="invalid_selection">Invalid selection</string>

    <!-- Default metadata text if a favorite has no metadata. This text should indicate to the user
         that the station is favorited followed by the number of the favorite. -->
    <string name="radio_default_favorite_metadata_text">Favorite <xliff:g>%1$s</xliff:g></string>

    <!-- The digits for the dialpad in the manual tuner. There is no need to localize them. -->
    <string name="manual_tuner_0">0</string>
    <string name="manual_tuner_1">1</string>
    <string name="manual_tuner_2">2</string>
    <string name="manual_tuner_3">3</string>
    <string name="manual_tuner_4">4</string>
    <string name="manual_tuner_5">5</string>
    <string name="manual_tuner_6">6</string>
    <string name="manual_tuner_7">7</string>
    <string name="manual_tuner_8">8</string>
    <string name="manual_tuner_9">9</string>
    <string name="manual_tuner_period">.</string>

    <!-- Tab header text. -->
    <string name="browse_tab">Browse</string>
    <string name="favorites_tab">Favorites</string>
    <string name="tune_tab">Tune</string>

    <!-- Text to confirm manual tuner selection. -->
    <string name="enter_text">Enter</string>
</resources>
